[Unit]
Description=Telegram Bot for Kakao Vision API
After=syslog.target
After=network.target

[Service]
Type=simple
User=some_user
Group=some_user
WorkingDirectory=/path/to/telegram-bot-kakao-vision
ExecStart=/path/to/telegram-bot-kakao-vision/telegram-bot-kakao-vision
Restart=always
RestartSec=5
Environment=

[Install]
WantedBy=multi-user.target
